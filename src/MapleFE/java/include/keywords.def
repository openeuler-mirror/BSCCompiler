// Please refer shared/token.h for the Keyword types.
// format KEYWORD(keyword, id, type)
 
// data types.
// class/enum/package are all considered data types.
KEYWORD(boolean,  Boolean,   KW_DT)
KEYWORD(byte,     Byte,      KW_DT)
KEYWORD(char,     Char,      KW_DT)
KEYWORD(class,    Class,     KW_DT)
KEYWORD(double,   Double,    KW_DT)
KEYWORD(enum,     Enum,      KW_DT)
KEYWORD(float,    Float,     KW_DT)
KEYWORD(int,      Int,       KW_DT)
KEYWORD(interface,Interface, KW_DT)
KEYWORD(long,     Long,      KW_DT)
KEYWORD(package,  Package,   KW_DT)
KEYWORD(short,    Short,     KW_DT)
KEYWORD(void,     Void,      KW_DT)
// var is added in java 10 to declare local variables, similar to auto in C++
KEYWORD(var,      Var,       KW_DT)

KEYWORD(break,    Break,     KW_CT)
KEYWORD(case,     Case,      KW_CT)
KEYWORD(catch,    Catch,     KW_CT)
KEYWORD(continue, Continue,  KW_CT)
KEYWORD(default,  Default,   KW_CT)
KEYWORD(do,       Do,        KW_CT)
KEYWORD(else,     Else,      KW_CT)
KEYWORD(finally,  Finally,   KW_CT)
KEYWORD(for,      For,       KW_CT)
KEYWORD(goto,     Goto,      KW_CT)
KEYWORD(if,       If,        KW_CT)
KEYWORD(return,   Return,    KW_CT)
KEYWORD(switch,   Switch,    KW_CT)
KEYWORD(try,      Try,       KW_CT)
KEYWORD(while,    While,     KW_CT)

// Data attributes
KEYWORD(abstract, Abstract,  KW_DA)
KEYWORD(const,    Const,     KW_DA)
KEYWORD(volatile, Volatile,  KW_DA)

// functions
KEYWORD(assert,    Assert,    KW_FN)
KEYWORD(new,       New,       KW_FN)

// language specific 
KEYWORD(instanceof,   Instanceof,  KW_SP)
KEYWORD(extends,      Extends,     KW_SP)
KEYWORD(implements,   Implements,  KW_SP)
KEYWORD(import,       Import,      KW_SP)
KEYWORD(super,        Super,       KW_SP)
KEYWORD(synchronized, Synchronized,KW_SP)
KEYWORD(this,         This,        KW_SP)
KEYWORD(throw,        Throw,       KW_SP)
KEYWORD(transient,    Transient,   KW_SP)

// function attributes
KEYWORD(final,    Final,     KW_FA)
KEYWORD(native,   Native,    KW_FA)
KEYWORD(private,  Private,   KW_FA)
KEYWORD(protected,Protected, KW_FA)
KEYWORD(public,   Public,    KW_FA)
KEYWORD(static,   Static,    KW_FA|KW_DA)  // both data and function attr
KEYWORD(strictfp, Strictfp,  KW_FA)


