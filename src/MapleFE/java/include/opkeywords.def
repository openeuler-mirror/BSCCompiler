// Arithmetic, Arithmetic
OPKEYWORD("+", Add, Add)
OPKEYWORD("-", Sub, Sub)
OPKEYWORD("*", Mul, Mul)
OPKEYWORD("/", Div, Div)
OPKEYWORD("%", Mod, Mod)
OPKEYWORD("++", Inc, Inc)
OPKEYWORD("--", Dec, Dec)
// Relation, Relation
OPKEYWORD("==", EQ, Eq)
OPKEYWORD("!=", NE, Ne)
OPKEYWORD(">", GT, Gt)
OPKEYWORD("<", LT, Lt)
OPKEYWORD(">=", GE, Ge)
OPKEYWORD("<=", LE, Le)
// Bitwise, Bitwise
OPKEYWORD("&", Band, Band)
OPKEYWORD("|", Bor, Bor)
OPKEYWORD("^", Bxor, Bxor)
OPKEYWORD("~", Bcomp, Bcomp)
// Shift, Shift
OPKEYWORD("<<", Shl, Shl)
OPKEYWORD(">>", Shr, Shr)
OPKEYWORD(">>>", Zext, Zext)
// Logical, Logical
OPKEYWORD("&&", Land, Land)
OPKEYWORD("||", Lor, Lor)
OPKEYWORD("!", Not, Not)
// Assign, Assign
OPKEYWORD("=", Assign, Assign)
OPKEYWORD("+=", AddAssign, Addassign)
OPKEYWORD("-=", SubAssign, Subassign)
OPKEYWORD("*=", MulAssign, Mulassign)
OPKEYWORD("/=", DivAssign, Divassign)
OPKEYWORD("%=", ModAssign, Modassign)
OPKEYWORD("<<=", ShlAssign, Shlassign)
OPKEYWORD(">>=", ShrAssign, Shrassign)
OPKEYWORD("&=", BandAssign, Bandassign)
OPKEYWORD("|=", BorAssign, Borassign)
OPKEYWORD("^=", BxorAssign, Bxorassign)
OPKEYWORD(">>>=", ZextAssign, Zextassign)
// others 
OPKEYWORD("->", Arrow, Arrow)
OPKEYWORD(":", Select, Select)
OPKEYWORD("?", Cond, Cond)
// OPKEYWORD("@", At, At)
// OPKEYWORD("::", Of, Of)
OPKEYWORD("<>", Diamond, Diamond)
